{
  "name": "@crisislens/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:e2e": "PLAYWRIGHT_PORT=3100 pnpm exec playwright test",
    "test:e2e:headed": "PLAYWRIGHT_PORT=3100 pnpm exec playwright test --headed",
    "test:e2e:ui": "PLAYWRIGHT_PORT=3100 pnpm exec playwright test tests/e2e/dashboard.spec.js tests/e2e/landing.spec.js",
    "test:e2e:api": "PLAYWRIGHT_PORT=3100 pnpm exec playwright test tests/e2e/api-routes.spec.js",
    "playwright:install": "pnpm exec playwright install chromium",
    "test": "pnpm run lint && pnpm run typecheck",
    "test:all": "pnpm run test && pnpm run test:unit && pnpm run test:e2e",
    "generate:data": "node scripts/generate-country-metrics.mjs"
  },
  "dependencies": {
    "@mediapipe/tasks-vision": "^0.10.32",
    "@types/three": "^0.183.1",
    "d3-geo": "^3.1.1",
    "framer-motion": "^12.34.3",
    "next": "14.2.32",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-globe.gl": "^2.37.0",
    "three": "^0.183.1",
    "three-globe": "^2.45.0",
    "topojson-client": "^3.1.0",
    "world-atlas": "^2.0.2",
    "world-countries": "^5.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/d3-geo": "^3.1.0",
    "@types/geojson": "^7946.0.16",
    "@types/node": "^20.17.57",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "@types/topojson-client": "^3.1.5",
    "autoprefixer": "^10.4.24",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.32",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vitest": "^4.0.18"
  }
}
